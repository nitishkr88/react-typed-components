{
  "name": "react-typed-components",
  "version": "1.0.0",
  "description": "Yet another Design System",
  "repository": "git+ssh://git@github.com/nitishkr88/react-typed-components.git",
  "homepage": "https://github.com/nitishkr88/react-typed-components#readme",
  "main": "lib/index.js",
  "module": "es6/index.js",
  "scripts": {
    "styleguide": "styleguidist server",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "build:clean": "rimraf es6 lib dist",
    "prebuild": "npm run typecheck && npm run build:clean",
    "build": "npm run build:es6 && npm run build:cjs && npm run build:umd",
    "build:es6": "tsc --module es2015 --target es2015 --outDir es6",
    "build:cjs": "tsc --module commonjs --target es5 --outDir lib",
    "build:umd": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "lint": "eslint --ignore-path .gitignore",
    "lint:fix": "eslint --ignore-path .gitignore --fix",
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "test:watch": "npm run test -- --watch",
    "test:cov": "npm run test -- --coverage",
    "prettify": "prettier \"+(src)/**/*.+(js|jsx|ts|tsx)\" --write",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "Nitish",
  "license": "MIT",
  "lint-staged": {
    "*.{js|jsx|ts|tsx}": [
      "npm run lint:fix",
      "git add --force"
    ],
    "*.{md,json}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "antd": "^3.26.6",
    "classnames": "^2.2.6",
    "color": "^3.1.2",
    "jest-watch-typeahead": "^0.4.2",
    "less-to-json-loader": "^1.1.0",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-grid-layout": "^0.17.1",
    "react-hot-loader": "^4.12.18",
    "recharts": "^1.8.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-syntax-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.25",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-grid-layout": "^0.17.0",
    "@types/recharts": "^1.8.5",
    "@types/webpack": "^4.41.1",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-polyfill": "^6.26.0",
    "camelcase": "^5.3.1",
    "compression-webpack-plugin": "^3.0.1",
    "copyfiles": "^2.1.1",
    "cpy-cli": "^3.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^4.5.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^1.19.1",
    "react-app-polyfill": "^1.0.5",
    "react-styleguidist": "^8.0.5",
    "rimraf": "^3.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.2",
    "stylelint": "^12.0.1",
    "stylelint-config-standard": "^19.0.0",
    "svg-url-loader": "^3.0.3",
    "terser-webpack-plugin": "^2.3.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
